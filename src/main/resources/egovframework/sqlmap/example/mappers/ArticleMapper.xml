<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.example.dao.ArticleDao">
    <select id="findAllArticle" resultType="egovframework.example.dto.ArticleDto">
        select id, title, memberid from study.article
    </select>
    <select id="findById"  resultType="egovframework.example.dto.ArticleDto">
        select * from study.article where id = #{id}
    </select>

</mapper>